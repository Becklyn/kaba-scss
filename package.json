        "prepublishOnly": "npm run-script build"
    },
    "devDependencies": {
        "@types/chokidar": "^1.7.5",
        "@types/fs-extra": "^5.0.4",
        "@types/node": "^10.12.21",
        "@types/sass": "^1.16.0",
        "@types/stylelint": "^9.4.1",
        "source-map": "^0.7.3",
        "typescript": "^3.3.1"
    },
    "engines": {
        "node": ">=8.0"
    }
}
